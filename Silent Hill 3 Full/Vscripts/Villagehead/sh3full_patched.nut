function CountdownTimer(amount)
{
	local i = amount;
	local j;
	for(j = amount; j > 0; j--)
	{
		EntFire("Channel 1","SetText",s1 + j.tostring() + s2,i-j);
		EntFire("Channel 1","Display","",i-j);
	}
}

